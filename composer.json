{
    "name": "shrikeh/collections",
    "type": "library",
    "description": "Traits for re-use in immutable collections",
    "keywords": ["library"],
    "license": "MIT",
    "authors": [
        {
            "name": "Barney Hanlon",
            "email": "barney@shrikeh.net"
        }
    ],
    "require-dev": {
      "phpspec/phpspec": "^3.1",
      "squizlabs/php_codesniffer": "^2.7",
      "escapestudios/symfony2-coding-standard": "^2.9",
      "phpmd/phpmd": "^2.5",
      "symfony/var-dumper": "^3.1"
    },
    "config": {
      "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "autoload": {
      "psr-4": {
        "Shrikeh\\Collection\\": "src",
        "Shrikeh\\Collection\\Examples\\": "examples"
      }
    },
    "archive": {
      "exclude": [
        "/examples",
        "/spec",
        "/phpspec.yml",
        "/.*"
      ]
    }
}
